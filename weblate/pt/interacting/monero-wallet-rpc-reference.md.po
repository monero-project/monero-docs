#. type: Title ###
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1315
#, no-wrap
msgid "**get_transfers**"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1318
msgid "Returns a list of transfers."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid "_in_ - boolean; (Optional) Include incoming transfers."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid "_out_ - boolean; (Optional) Include outgoing transfers."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid "_pending_ - boolean; (Optional) Include pending transfers."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid "_failed_ - boolean; (Optional) Include failed transfers."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid ""
"_pool_ - boolean; (Optional) Include transfers from the daemon's transaction "
"pool."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid ""
"_filter_by_height_ - boolean; (Optional) Filter transfers by block height."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid ""
"_min_height_ - unsigned int; (Optional) Minimum block height to scan for "
"transfers, if filtering by height is enabled."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid ""
"_max_height_ - unsigned int; (Optional) Maximum block height to scan for "
"transfers, if filtering by height is enabled (defaults to max block height)."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid ""
"_account_index_ - unsigned int; (Optional) Index of the account to query for "
"transfers. (defaults to 0)"
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1333
msgid ""
"_subaddr_indices_ - array of unsigned int; (Optional) List of subaddress "
"indices to query for transfers. (defaults to 0)"
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_in_ array of transfers:"
msgstr ""

#. type: Bullet: '    -   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_address_ - string; Public address of the transfer."
msgstr ""

#. type: Bullet: '    -   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_amount_ - unsigned int; Amount transferred."
msgstr ""

#. type: Bullet: '    -   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid ""
"_height_ - unsigned int; Height of the first block that confirmed this "
"transfer (0 if not mined yet)."
msgstr ""

#. type: Bullet: '    -   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid ""
"_timestamp_ - unsigned int; POSIX timestamp for when this transfer was first "
"confirmed in a block (or timestamp submission if not mined yet)."
msgstr ""

#. type: Bullet: '    -   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_txid_ - string; Transaction ID for this transfer."
msgstr ""

#. type: Bullet: '    -   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_type_ - string; Transfer type: \"in\""
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_out_ array of transfers (see above)."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_pending_ array of transfers (see above)."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_failed_ array of transfers (see above)."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:1357
msgid "_pool_ array of transfers (see above)."
msgstr ""

