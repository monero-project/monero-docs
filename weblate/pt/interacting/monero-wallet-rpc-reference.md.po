"$ curl -X POST http://localhost:18082/json_rpc -d '{\"jsonrpc\":\"2.0\",\"id\":\"0\",\"method\":\"sweep_single\",\"params\":{\"address\":\"74Jsocx8xbpTBEjm3ncKE5LBQbiJouyCDaGhgSiebpvNDXZnTAbW2CmUR5SsBeae2pNk9WMVuz6jegkC4krUyqRjA6VjoLD\",\"ring_size\":7,\"unlock_time\":0,\"key_image\":\"a7834459ef795d2efb6f665d2fd758c8d9288989d8d4c712a68f8023f7804a5e\",\"get_tx_keys\":true}}' -H 'Content-Type: application/json'\n"
"{\n"
"  \"id\": \"0\",\n"
"  \"jsonrpc\": \"2.0\",\n"
"  \"result\": {\n"
"    \"amount\": 27126892247503,\n"
"    \"fee\": 14111630000,\n"
"    \"multisig_txset\": \"\",\n"
"    \"tx_blob\": \"\",\n"
"    \"tx_hash\": \"106d4391a031e5b735ded555862fec63233e34e5fa4fc7edcfdbe461c275ae5b\",\n"
"    \"tx_key\": \"\",\n"
"    \"tx_metadata\": \"\",\n"
"    \"unsigned_txset\": \"\"\n"
"  }\n"
"}\n"
"\n"
msgstr ""

#. type: Title ###
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2661
#, no-wrap
msgid "**tag_accounts**"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2664
msgid "Apply a filtering tag to a list of accounts."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2671
msgid "_tag_ - string; Tag for the accounts."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2671
msgid "_accounts_ - array of unsigned int; Tag this list of accounts."
msgstr ""

#. type: Fenced code block (Bash)
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2676
#, no-wrap
msgid ""
"$ curl -X POST http://localhost:18082/json_rpc -d '{\"jsonrpc\":\"2.0\",\"id\":\"0\",\"method\":\"tag_accounts\",\"params\":{\"tag\":\"myTag\",\"accounts\":[0,1]}}' -H 'Content-Type: application/json'\n"
"{\n"
"  \"id\": \"0\",\n"
"  \"jsonrpc\": \"2.0\",\n"
"  \"result\": {\n"
"  }\n"
"}\n"
"\n"
msgstr ""

#. type: Title ###
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2688
#, no-wrap
msgid "**transfer**"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2691
msgid "Send monero to a number of recipients."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid "_destinations_ - array of destinations to receive XMR:"
msgstr ""

#. type: Bullet: '    -   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid ""
"_amount_ - unsigned int; Amount to send to each destination, in atomic units."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid ""
"_account_index_ - unsigned int; (Optional) Transfer from this account index. "
"(Defaults to 0)"
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid ""
"_subaddr_indices_ - array of unsigned int; (Optional) Transfer from this set "
"of subaddresses. (Defaults to 0)"
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
msgid ""
"_priority_ - unsigned int; Set a priority for the transaction. Accepted "
"Values are: 0-3 for: default, unimportant, normal, elevated, priority."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
msgid ""
"_ring_size_ - unsigned int; Number of outputs to mix in the transaction "
"(this output + N decoys from the blockchain)."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
msgid ""
"_get_tx_key_ - boolean; (Optional) Return the transaction key after sending."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
msgid ""
"_get_tx_hex_ - boolean; Return the transaction as hex string after sending "
"(Defaults to false)"
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2710
msgid ""
"_get_tx_metadata_ - boolean; Return the metadata needed to relay the "
"transaction. (Defaults to false)"
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2721
msgid "_amount_ - Amount transferred for the transaction."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2721
msgid "_fee_ - Integer value of the fee charged for the txn."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2721
msgid ""
"_multisig_txset_ - Set of multisig transactions in the process of being "
"signed (empty for non-multisig)."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2721
msgid ""
"_tx_blob_ - Raw transaction represented as hex string, if get_tx_hex is true."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2721
msgid ""
"_tx_key_ - String for the transaction key if get_tx_key is true, otherwise, "
"blank string."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2721
msgid ""
"_tx_metadata_ - Set of transaction metadata needed to relay this transfer "
"later, if get_tx_metadata is true."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2721
msgid ""
"_unsigned_txset_ - String. Set of unsigned tx for cold-signing purposes."
msgstr ""

#. type: Fenced code block (Bash)
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2724
#, no-wrap
msgid ""
"$ curl -X POST http://127.0.0.1:18082/json_rpc -d '{\"jsonrpc\":\"2.0\",\"id\":\"0\",\"method\":\"transfer\",\"params\":{\"destinations\":[{\"amount\":100000000000,\"address\":\"7BnERTpvL5MbCLtj5n9No7J5oE5hHiB3tVCK5cjSvCsYWD2WRJLFuWeKTLiXo5QJqt2ZwUaLy2Vh1Ad51K7FNgqcHgjW85o\"},{\"amount\":200000000000,\"address\":\"75sNpRwUtekcJGejMuLSGA71QFuK1qcCVLZnYRTfQLgFU5nJ7xiAHtR5ihioS53KMe8pBhH61moraZHyLoG4G7fMER8xkNv\"}],\"account_index\":0,\"subaddr_indices\":[0],\"priority\":0,\"ring_size\":7,\"get_tx_key\": true}}' -H 'Content-Type: application/json'\n"
"{\n"
"  \"id\": \"0\",\n"
"  \"jsonrpc\": \"2.0\",\n"
"  \"result\": {\n"
"    \"amount\": 300000000000,\n"
"    \"fee\": 86897600000,\n"
"    \"multisig_txset\": \"\",\n"
"    \"tx_blob\": \"\",\n"
"    \"tx_hash\": \"7663438de4f72b25a0e395b770ea9ecf7108cd2f0c4b75be0b14a103d3362be9\",\n"
"    \"tx_key\": \"25c9d8ec20045c80c93d665c9d3684aab7335f8b2cd02e1ba2638485afd1c70e236c4bdd7a2f1cb511dbf466f13421bdf8df988b7b969c448ca6239d7251490e4bf1bbf9f6ffacffdcdc93b9d1648ec499eada4d6b4e02ce92d4a1c0452e5d009fbbbf15b549df8856205a4c7bda6338d82c823f911acd00cb75850b198c5803\",\n"
"    \"tx_metadata\": \"\",\n"
"    \"unsigned_txset\": \"\"\n"
"  }\n"
"}\n"
"\n"
msgstr ""

#. type: Title ###
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2744
#, no-wrap
msgid "**transfer_split**"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2747
msgid "Same as transfer, but can split into more than one tx if necessary."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid ""
"_priority_ - unsigned int; Set a priority for the transactions. Accepted "
"Values are: 0-3 for: default, unimportant, normal, elevated, priority."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid ""
"_get_tx_hex_ - boolean; Return the transactions as hex string after sending"
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid ""
"_new_algorithm_ - boolean; True to use the new transaction construction "
"algorithm, defaults to false."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2767
msgid ""
"_get_tx_metadata_ - boolean; Return list of transaction metadata needed to "
"relay the transfer later."
msgstr ""

#. type: Fenced code block (Bash)
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2781
#, no-wrap
msgid ""
"$ curl -X POST http://127.0.0.1:18082/json_rpc -d '{\"jsonrpc\":\"2.0\",\"id\":\"0\",\"method\":\"transfer_split\",\"params\":{\"destinations\":[{\"amount\":1000000000000,\"address\":\"7BnERTpvL5MbCLtj5n9No7J5oE5hHiB3tVCK5cjSvCsYWD2WRJLFuWeKTLiXo5QJqt2ZwUaLy2Vh1Ad51K7FNgqcHgjW85o\"},{\"amount\":2000000000000,\"address\":\"75sNpRwUtekcJGejMuLSGA71QFuK1qcCVLZnYRTfQLgFU5nJ7xiAHtR5ihioS53KMe8pBhH61moraZHyLoG4G7fMER8xkNv\"}],\"account_index\":0,\"subaddr_indices\":[0],\"priority\":0,\"ring_size\":7,\"get_tx_key\": true}}' -H 'Content-Type: application/json'\n"
"{\n"
"  \"id\": \"0\",\n"
"  \"jsonrpc\": \"2.0\",\n"
"  \"result\": {\n"
"    \"amount_list\": [3000000000000],\n"
"    \"fee_list\": [85106400000],\n"
"    \"multisig_txset\": \"\",\n"
"    \"tx_hash_list\": [\"c8d815f48f27d53fdaf198a74b292a91bfaf87529a9a9a9ee66079a890b3b58b\"],\n"
"    \"unsigned_txset\": \"\"\n"
"  }\n"
"}\n"
"\n"
msgstr ""

#. type: Title ###
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2798
#, no-wrap
msgid "**untag_accounts**"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2801
msgid "Remove filtering tag from a list of accounts."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2807
msgid ""
"_accounts_ - array of unsigned int; Remove tag from this list of accounts."
msgstr ""

#. type: Fenced code block (Bash)
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2812
#, no-wrap
msgid ""
"$ curl -X POST http://localhost:18082/json_rpc -d '{\"jsonrpc\":\"2.0\",\"id\":\"0\",\"method\":\"untag_accounts\",\"params\":{\"accounts\":[1]}}' -H 'Content-Type: application/json'\n"
"{\n"
"  \"id\": \"0\",\n"
"  \"jsonrpc\": \"2.0\",\n"
"  \"result\": {\n"
"  }\n"
"}\n"
"\n"
msgstr ""

#. type: Title ###
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2824
#, no-wrap
msgid "**verify**"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2827
msgid "Verify a signature on a string."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2835
msgid "_data_ - string; What should have been signed."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2835
msgid ""
"_address_ - string; Public address of the wallet used to `sign` the data."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2835
msgid "_signature_ - string; signature generated by `sign` method."
msgstr ""

#. type: Bullet: '-   '
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2839
msgid "_good_ - boolean;"
msgstr ""

#. type: Fenced code block (Bash)
#: ../docs/en/interacting/monero-wallet-rpc-reference.md:2842
#, no-wrap
msgid ""
"$ curl -X POST http://127.0.0.1:18082/json_rpc -d '{\"jsonrpc\":\"2.0\",\"id\":\"0\",\"method\":\"verify\",\"params\":{\"data\":\"This is sample data to be signed\",\"address\":\"55LTR8KniP4LQGJSPtbYDacR7dz8RBFnsfAKMaMuwUNYX6aQbBcovzDPyrQF9KXF9tVU6Xk3K8no1BywnJX6GvZX8yJsXvt\",\"signature\":\"SigV14K6G151gycjiGxjQ74tKX6A2LwwghvuHjcDeuRFQio5LS6Gb27BNxjYQY1dPuUvXkEbGQUkiHSVLPj4nJAHRrrw3\"}}' -H 'Content-Type: application/json'\n"
"{\n"
"  \"id\": \"0\",\n"
"  \"jsonrpc\": \"2.0\",\n"
"  \"result\": {\n"
"    \"good\": true\n"
"  }\n"
"}\n"
"\n"
msgstr ""

