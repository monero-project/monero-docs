#. type: Title ##
#: ../docs/en/interacting/monero-config-file.md:47
#, markdown-text, no-wrap
msgid "Examples"
msgstr ""

#. type: Title ###
#: ../docs/en/interacting/monero-config-file.md:49
#, markdown-text, no-wrap
msgid "`monerod.conf`"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:52
#, markdown-text
msgid "This config is tailored for production server usage."
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:54
#, markdown-text, no-wrap
msgid "    # /etc/monero/monerod.conf\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:57
#, markdown-text, no-wrap
msgid ""
"    # Data directory (blockchain db and indices)\n"
"    data-dir=/home/monero/.monero  # Remember to create the monero user first\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:61
#, markdown-text, no-wrap
msgid ""
"    # Log file\n"
"    log-file=/var/log/monero/monerod.log\n"
"    max-log-file-size=0            # Prevent monerod from managing the log files; we want logrotate to take care of that\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:65
#, markdown-text, no-wrap
msgid ""
"    # P2P full node\n"
"    p2p-bind-ip=0.0.0.0            # Bind to all interfaces (the default)\n"
"    p2p-bind-port=18080            # Bind to default port\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:72
#, markdown-text, no-wrap
msgid ""
"    # RPC open node\n"
"    rpc-bind-ip=0.0.0.0            # Bind to all interfaces\n"
"    rpc-bind-port=18081            # Bind on default port\n"
"    confirm-external-bind=1        # Open node (confirm)\n"
"    restricted-rpc=1               # Prevent unsafe RPC calls\n"
"    no-igd=1                       # Disable UPnP port mapping\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:75
#, markdown-text, no-wrap
msgid ""
"    # Slow but reliable db writes\n"
"    db-sync-mode=safe\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:79
#, markdown-text, no-wrap
msgid ""
"    # Emergency checkpoints set by MoneroPulse operators will be enforced to workaround potential consensus bugs\n"
"    # Check https://monerodocs.org/infrastructure/monero-pulse/ for explanation and trade-offs\n"
"    enforce-dns-checkpointing=1\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:82
#, markdown-text, no-wrap
msgid ""
"    out-peers=64              # This will enable much faster sync and tx awareness; the default 8 is suboptimal nowadays\n"
"    in-peers=1024             # The default is unlimited; we prefer to put a cap on this\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:85
#, markdown-text, no-wrap
msgid ""
"    limit-rate-up=1048576     # 1048576 kB/s == 1GB/s; a raise from default 2048 kB/s; contribute more to p2p network\n"
"    limit-rate-down=1048576   # 1048576 kB/s == 1GB/s; a raise from default 8192 kB/s; allow for faster initial sync\n"
msgstr ""

#. type: Title ###
#: ../docs/en/interacting/monero-config-file.md:86
#, markdown-text, no-wrap
msgid "`monero-wallet-cli.conf`"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:89
#, markdown-text
msgid "This config is tailored for desktop usage on [stagenet](/infrastructure/networks)."
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:91
#, markdown-text, no-wrap
msgid "    # $HOME/.bitmonero/stagenet/monero-wallet-cli.conf\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:94
#, markdown-text, no-wrap
msgid ""
"    # Pick network\n"
"    stagenet=1\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:98
#, markdown-text, no-wrap
msgid ""
"    # Connect to a remote full node    \n"
"    daemon-address=monero-stagenet.exan.tech:38081\n"
"    untrusted-daemon=1\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:101
#, markdown-text, no-wrap
msgid ""
"    # Log file\n"
"    log-file=/tmp/monero-wallet-cli.log\n"
msgstr ""

#. type: Plain text
#: ../docs/en/interacting/monero-config-file.md:102
#, markdown-text, no-wrap
msgid "    # wallet-file=/home/YOUR-USER/.bitmonero/stagenet/wallets/MoneroExampleStagenetWallet\n"
msgstr ""
